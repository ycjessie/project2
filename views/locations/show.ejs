
    <h1><%= location.name %>'s Locations</h1>
<ul>
  <% location.address.forEach(add => { %>
  <li><%= add.cityName %>, <%= add.stateName %> </li>
  <% }) %>
</ul>
<h4>Create a New Store Location</h4>
<form action="/locations/<%= location.id %>/address" method="POST">
  <div class="form-group">

    <label for="cityName">City</label>
    <input required type="text" class="form-control" id="cityName" name="cityName"/>
    <label for="stateName">State</label>
    <input required type="text" class="form-control" id="stateName" name="stateName"/>
    
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
